	-r minimal epsilon [Default: (data interval)/1000]
	-R maximal epsilon [Default: data interval]
	-# # of epsilons to use [Default: 20]
	-o output file name [Default: 'datafile'.box]
	-V verbosity level [Default: 1]
		0='only panic messages'
		1='+ input/output messages'
